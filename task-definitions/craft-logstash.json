    {
      "name": "craft-logstash",
      "image": "033441544097.dkr.ecr.us-east-1.amazonaws.com/craft-logstash",
      "essential": true,
      "cpu": 128,
      "memory": 1024,
      "memoryReservation": 512,
      "ulimits": null,
      "portMappings": [
        {
          "hostPort": 20514,
