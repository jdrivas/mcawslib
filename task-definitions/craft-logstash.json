        "service": "craft-logstash"
      },
      "links": [],
      "mountPoints": [],
      "volumesFrom": [],
      "readonlyRootFilesystem": null,
      "logConfiguration": {
        "logDriver": "syslog",
        "options": {
          "syslog-address": "udp://0.0.0.0:10514"
        }
      },    
      "dockerSecurityOptions": null,
      "privileged": null,
      "extraHosts": null,
      "dnsServers": null,
      "disableNetworking": null,
      "dnsSearchDomains": null
